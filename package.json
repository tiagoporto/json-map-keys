{
  "name": "json-map-keys",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=23",
    "npm": "uses pnpm",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977",
  "scripts": {
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "$CI = true || (dependabot-config && editor-config)",
    "check-docs": "remark --frail . --ext md,markdown,mdx",
    "checks": "pnpm check-docs && pnpm format:check && pnpm lint",
    "format": "pnpm format:check --write",
    "format:check": "prettier --check --ignore-unknown \"**/*\"",
    "lint": "eslint --max-warnings 0",
    "lint:fix": "pnpm lint --fix",
    "coverage": "vitest run --coverage",
    "test": "vitest",
    "test:mutation": "stryker run",
    "webpack-loader": "webpack",
    "rollup-plugin": "rollup --config",
    "jest-transformer": "jest",
    "release": "multi-semantic-release",
    "release:dry-run": "multi-semantic-release --dry-run"
  },
  "dependencies": {
    "json-map-keys": "workspace:*",
    "json-map-keys-jest": "workspace:*",
    "json-map-keys-loader": "workspace:*",
    "rollup-plugin-json-map-keys": "workspace:*"
  },
  "devDependencies": {
    "@anolilab/multi-semantic-release": "^2.0.5",
    "@anolilab/semantic-release-pnpm": "^2.0.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^20.0.0",
    "@sebbo2002/semantic-release-jsr": "^3.0.1",
    "@stryker-mutator/core": "^9.2.0",
    "@stryker-mutator/typescript-checker": "^9.1.1",
    "@stryker-mutator/vitest-runner": "^9.1.1",
    "@swc/jest": "^0.2.39",
    "@tiagoporto/dependabot-config": "^2.4.0",
    "@tiagoporto/editor-config": "^2.0.0",
    "@tiagoporto/eslint-config": "^5.3.0",
    "@tiagoporto/prettier-config": "^2.1.0",
    "@tiagoporto/remark-config": "^1.11.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.10",
    "eslint": "^9.36.0",
    "husky": "^9.1.7",
    "jest": "^27.0.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "remark": "^15.0.1",
    "remark-cli": "^12.0.1",
    "rollup": "^4.52.4",
    "semantic-release": "^24.2.9",
    "vitest": "^3.2.4",
    "webpack": "^5.102.0",
    "webpack-cli": "^6.0.1"
  },
  "pnpm": {
    "overrides": {
      "conventional-changelog-conventionalcommits": ">=8.0.0"
    }
  }
}
