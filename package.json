{
  "name": "json-map-keys",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "lint": "",
    "check-links": "markdown-link-check *.md && markdown-link-check packages/json-map-keys/*.md && markdown-link-check packages/json-map-keys-jest/*.md && markdown-link-check packages/json-map-keys-loader/*.md && markdown-link-check packages/json-map-keys-rollup/*.md",
    "checks": "npm run check-formatting",
    "check-formatting": "prettier --check --ignore-unknown \"**/*\"",
    "fix:formatting": "npm run check-formatting -- --write",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "test:mutation": "stryker run",
    "test:mutation-report": "stryker run --reporters dashboard",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "postpublish": "npm run tag && git push --tags",
    "tag": "git tag v$npm_package_version"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^4.3.1",
    "@stryker-mutator/javascript-mutator": "^4.0.0",
    "@stryker-mutator/jest-runner": "^4.3.1",
    "coveralls": "^3.1.0",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "markdown-link-check": "^3.8.6",
    "prettier": "^2.2.1"
  }
}
